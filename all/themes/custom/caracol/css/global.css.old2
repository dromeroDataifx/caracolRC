.aparece {
   display: block;
   animation-duration: 1s;
   animation-name: aparece-lateral;
   position: relative;
}
.desaparece {
   display: none;
   animation-duration: 1s;
   animation-name: desaparecer-lateral;
   position: relative;

}
@Keyframes desaparecer-lateral {
    from {
        left: 0px;
        opacity: 0.5;
    }
    to {
        left: -100px;
        opacity: 0;
    }
}
@Keyframes aparece-lateral {
    from {
        left: 100px;
        opacity: 0.5;
    }
    to {
        left: 0px;
        opacity: 1;
    }
}

